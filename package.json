{
  "homepage": "https://nh60211as.github.io/TaiwanVtuberData-Preact",
  "private": true,
  "name": "preact-deploy",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "build:gh": "GITHUB_PAGES=TaiwanVtuberData-Preact preact build && dot-json ./build/manifest.json start_url \"/TaiwanVtuberData-Preact/\"",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --config .prettierrc src/**/*.{js,jsx,ts,tsx,css,json} --write",
    "test": "jest ./tests"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "preact",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "build/"
    ]
  },
  "dependencies": {
    "axios": "^0.26.1",
    "miragejs": "^0.1.43",
    "preact": "^10.3.1",
    "preact-i18n": "^2.4.0-preactx",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^4.0.1",
    "react-data-table-component": "^7.4.7",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.8",
    "@types/preact-i18n": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "dot-json": "^1.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-preact": "^1.1.1",
    "jest": "^27.2.5",
    "jest-preset-preact": "^4.0.2",
    "preact-cli": "^3.0.0",
    "prettier": "^2.5.1",
    "sirv-cli": "^1.0.0-next.3",
    "typescript": "^4.5.2"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.ts",
      "<rootDir>/tests/__mocks__/setupTests.ts"
    ]
  }
}
